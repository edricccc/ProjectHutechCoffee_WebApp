    <script src="~/Content/js/lib/easing/easing.min.js"></script>
    <script src="~/Content/js/lib/waypoints/waypoints.min.js"></script>
    <script src="~/Content/js/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Content/js/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="~/Content/js/lib/lightbox/js/lightbox.min.js"></script>

