    <script src="~/Content/js/mail/jqBootstrapValidation.min.js"></script>
    <script src="~/Content/js/mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="~/Content/js/main.js"></script>
    @RenderSection("Scripts", required: false)
